  },
  "homepage": "https://github.com/diogofcunha/aws-workshop#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "8.0.5",
    "babel-polyfill": "^6.26.0",
    "nodemon": "^1.19.1",
    "serverless-http": "^2.0.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
